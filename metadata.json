  "description": "A module for installing and configuring the Icinga 2 monitoring system",
  "dependencies": [
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 1.5.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.3.2"
    }
  ]
}
